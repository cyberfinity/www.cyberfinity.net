{%- from '@page-header' import pageHeader -%}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link media="all" rel="stylesheet" href="{{ '/css/style.css' | path }}">
  <title>{{ title }}</title>
</head>
<body>
  {% block pageHeader %}
    {{ pageHeader(title, navLinks, currentUrl) | safe }}
  {% endblock %}
  <main>
    {% block pageMain %}
      <h1>Heading 1</h1>
      <h2>Heading 2</h2>
      <h3>Heading 3</h3>
      <h4>Heading 4</h4>
      <h5>Heading 5</h5>
      <h6>Heading 6</h6>
      <p>Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text. Paragraph text.</p>
    {% endblock %}
  </main>
  <footer>
    {% block pageFooter %}

    {% endblock %}
  </footer>
</body>
</html>

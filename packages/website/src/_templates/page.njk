{%- from '@page-header.njk' import pageHeader -%}
{%- set navLinks = collections.all | eleventyNavigation -%}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link media="all" rel="stylesheet" href="{{ '/style/css/style.css' }}">
  <title>{{ title }}</title>
</head>
<body>
{% block body %}
  {{ pageHeader(title, navLinks, page.url) | safe }}
  <main>
  {% block main %}
    {{ content | safe }}
  {% endblock %}
  </main>
  {% include 'partials/page-footer.njk' %}
{% endblock %}
</body>
</html>
